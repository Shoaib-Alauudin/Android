<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ghulam.miwok_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MiwokActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ColorsActivity"
            android:label="@string/Colors_textView"
            android:parentActivityName=".MiwokActivity" />
        <activity
            android:name=".FamilyActivity"
            android:label="@string/family_members_textView"
            android:parentActivityName=".MiwokActivity" />
        <activity
            android:name=".NumbersActivity"
            android:label="@string/numbers_textView"
            android:parentActivityName=".MiwokActivity" />
        <activity
            android:name=".PhrasesActivity"
            android:label="@string/phrases_textView"
            android:parentActivityName=".MiwokActivity" />
    </application>

</manifest>